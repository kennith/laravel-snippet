{
	"name": "laravel-snippets",
	"displayName": "Laravel Snippets",
	"description": "A Laravel snippets for Visual Studio Code.",
	"homepage": "https://github.com/kennith/laravel-snippets",
	"bugs": {"url": "https://github.com/kennith/laravel-snippets/issues"},
	"keywords" :["laravel snippets", "laravel", "snippets"],
	"version": "1.0.0",
	"license": "LICENSE.md",
	"publisher": "kennith",
	"repository": {
		"type": "git",
		"url": "https://github.com/kennith/laravel-snippets.git"
	  },
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [
	],
	"main": "./out/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "php",
				"path": "./snippets/helpers/array.json"
			},
			{
				"language": "php",
				"path": "./snippets/helpers/str.json"
			},
			{
				"language": "php",
				"path": "./snippets/helpers/path.json"
			},
			{
				"language": "php",
				"path": "./snippets/helpers/url.json"
			},
			{
				"language": "php",
				"path": "./snippets/helpers/misc.json"
			},
			{
				"language": "php",
				"path": "./snippets/helpers/fluent-string.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"deploy": "vsce publish --yarn -p $VSCE_PAT"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}